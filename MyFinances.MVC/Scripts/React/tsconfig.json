{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "module": "esnext",
    "importHelpers": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "./dist",
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2015.core",
      "es2015.promise",
      "es2015.collection",
      "es2016.array.include",
      "es2017.string",
      "es2017.object",
      "es2018.promise",
      "es2015.reflect",
      "es2017.typedarrays"
    ],
    "jsx": "react"
  },
  "include": [
    "./Typescript/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}